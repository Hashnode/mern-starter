{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
   },
   "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
   },
   "rules": {
     "comma-dangle": [1, "never"],
     "max-len": [1, 999, 4],
     "arrow-body-style": [0]
   }
}
